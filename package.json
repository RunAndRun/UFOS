{
  "name": "UFOS",
  "version": "0.1.0",
  "keywords": [
    "ufos",
    "UFOS"
  ],
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "server": "nodemon src/server/server.js",
    "dev": "concurrently \"yarn serve\" \"yarn server\"",
    "build": "vue-cli-service build",
    "fix": "eslint --fix --ext .js --ext .jsx --ext .vue src/",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "dayjs": "^1.8.14",
    "iview": "^3.4.2",
    "js-md5": "^0.7.3",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-generic-session": "^2.0.1",
    "koa-json": "^2.0.2",
    "koa-passport": "^4.1.3",
    "koa-redis": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "mongoose": "^5.6.0",
    "nodemailer": "^6.2.1",
    "passport-local": "^1.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.12.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "glob-all": "^3.1.0",
    "node-sass": "^7.0.0",
    "nodemon": "^1.19.1",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "vue/no-parsing-error": 0,
      "vue/valid-v-for": 0,
      "vue/valid-v-else": 0,
      "no-path-concat": 0,
      "new-cap": 0,
      "vue/no-duplicate-attributes": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
